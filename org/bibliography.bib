@article{lehman18,
  author =       {Lehman, Joel and Clune, Jeff and Misevic, Dusan and Adami,
                  Christoph and Beaulieu, Julie and Bentley, Peter J. and
                  Bernard, Samuel and Belson, Guillaume and Bryson, David M. and
                  Cheney, Nick and Cully, Antoine and Donciuex, Stephane and
